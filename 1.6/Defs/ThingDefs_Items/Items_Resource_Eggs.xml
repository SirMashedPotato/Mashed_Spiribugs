<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ==========[Mashed_Spiribug_EggBase]========== -->
	<ThingDef ParentName="EggFertBase" Abstract="True" Name="Mashed_Spiribug_EggBase">
		<description>A fertilized Spiribug egg. If all goes well, it should hatch into a spiribug larva. It can be eaten raw, but it's much better cooked.</description>
		<graphicData>
			<texPath>Things/Item/Resource/Mashed_SpiribugEggs</texPath>
		</graphicData>
	</ThingDef>
	<!-- ==========[Mashed_Spiribug_EggUnfertilized]========== -->
	<ThingDef ParentName="EggUnfertBase">
		<defName>Mashed_Spiribug_EggUnfertilized</defName>
		<label>spiribug egg (unfert.)</label>
		<description>An unfertilized Spiribug egg. It can be eaten raw, but it's much better cooked.</description>
		<graphicData>
			<texPath>Things/Item/Resource/Mashed_SpiribugEggs</texPath>
		</graphicData>
		<statBases>
			<MarketValue>3</MarketValue>
		</statBases>
	</ThingDef>
	<!-- ==========[Mashed_Spiribug_EggSilverFertilized]========== -->
	<ThingDef ParentName="Mashed_Spiribug_EggBase">
		<defName>Mashed_Spiribug_EggSilverFertilized</defName>
		<label>gleaming spiribug egg (fert.)</label>
		<comps>
			<li Class="CompProperties_Hatcher">
				<hatcherDaystoHatch>3.5</hatcherDaystoHatch>
				<hatcherPawn>Mashed_Spiribug_SpiribugSilver</hatcherPawn>
			</li>
		</comps>
		<statBases>
			<MarketValue>5</MarketValue>
		</statBases>
	</ThingDef>
	<!-- ==========[Mashed_Spiribug_EggJadeFertilized]========== -->
