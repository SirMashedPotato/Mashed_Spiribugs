	<RecipeDef ParentName="Mashed_Spiribug_MeltDustBase">
		<defName>Mashed_Spiribug_DyeFromDustSilver</defName>
		<label>mix gleaming spiribug dust</label>
		<description>Create 2 dye from 4 gleaming spiribug dust.</description>
		<jobString>Mixing gleaming spirbug dust.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Mashed_Spiribug_DustSilver</li>
					</thingDefs>
				</filter>
				<count>4</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Mashed_Spiribug_DustSilver</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Dye>2</Dye>
		</products>
	</RecipeDef>
