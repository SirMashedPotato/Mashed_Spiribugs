<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ==========[Mashed_Spiribug_DyeFromDust]========== -->
	<RecipeDef ParentName="Mashed_Spiribug_MeltDustBase">
		<defName>Mashed_Spiribug_DyeFromDust</defName>
		<label>mix spiribug dust</label>
		<description>Create 4 dye from 2 spiribug dust.</description>
		<jobString>Mixing spirbug dust.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Mashed_Spiribug_DustSilver</li>
						<li>Mashed_Spiribug_DustJade</li>
						<li>Mashed_Spiribug_DustGold</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Mashed_Spiribug_DustSilver</li>
				<li>Mashed_Spiribug_DustJade</li>
				<li>Mashed_Spiribug_DustGold</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Dye>4</Dye>
		</products>
	</RecipeDef>
	<!-- ==========[Mashed_Spiribug_DyeFromDust_Bulk]========== -->
	<RecipeDef ParentName="Mashed_Spiribug_MeltDustBulkBase">
		<defName>Mashed_Spiribug_DyeFromDust_Bulk</defName>
		<label>mix spiribug dust x4</label>
		<description>Create 16 dye from 8 spiribug dust.</description>
		<jobString>Mixing spirbug dust.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Mashed_Spiribug_DustSilver</li>
						<li>Mashed_Spiribug_DustJade</li>
						<li>Mashed_Spiribug_DustGold</li>
					</thingDefs>
				</filter>
				<count>8</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Mashed_Spiribug_DustSilver</li>
				<li>Mashed_Spiribug_DustJade</li>
				<li>Mashed_Spiribug_DustGold</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Dye>16</Dye>
		</products>
	</RecipeDef>
</Defs>
